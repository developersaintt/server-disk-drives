        "template--17031626588404__3d66d386-0b61-4f22-a10c-ef7989481cc0-question-1": {
          "type": "question",
          "settings": {
            "title": "How do I change my shipping address?",
            "answer": "<p>Shipping address can be changed before the order is marked as shipped by contacting us with the order number<\/p>"
          }
        },
        "template--17031626588404__3d66d386-0b61-4f22-a10c-ef7989481cc0-question-2": {
          "type": "question",
          "settings": {
            "title": "What do I do if I entered an incorrect shipping address?",
            "answer": "<p>Contact us with correct address however please note that shipping address can not be changed once order has been marked shipped. <\/p>"
          }
        },
        "template--17031626588404__3d66d386-0b61-4f22-a10c-ef7989481cc0-question-3": {
          "type": "question",
          "settings": {
            "title": "Do you ship to my country?",
            "answer": "<p>We ship to most International destinations as required by laws in the United States. Please contact us for a quote.<\/p>"
          }
        },
        "template--17031626588404__3d66d386-0b61-4f22-a10c-ef7989481cc0-separator-2": {
